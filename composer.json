{
    "name": "jhofm/ph-puml",
    "description": "Generates PlantUML class diagrams from PHP code",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Hofmann",
            "email": "hofmann.johannes@gmail.com"
        }
    ],
    "bin": ["bin/ph-puml"],
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7.1"
    },
    "require": {
        "php": "^8.2",
        "composer-plugin-api": "^2.0",
        "ext-json": "*",
        "symfony/console": "^6.2.2",
        "nikic/php-parser": "^4.15.2",
        "symfony/dependency-injection": "^6.2.3",
        "symfony/process": "^6.2.0",
        "symfony/finder": "^5.2",
        "symfony/config": "^6.2.0",
        "symfony/yaml": "^6.2.2",
        "jawira/plantuml": "^v1.2022.14"
    },
    "autoload": {
        "psr-4": {
            "Jhofm\\PhPuml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhofm\\PhPuml\\Test\\": "tests"
        }
    }
}

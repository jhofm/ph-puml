{
    "name": "jhofm/ph-puml",
    "description": "Generates PlantUML class diagrams from PHP code",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Hofmann",
            "email": "hofmann.johannes@gmail.com"
        }
    ],
    "bin": ["bin/ph-puml"],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require": {
        "php": "^7.4",
        "composer-plugin-api": "^2.0",
        "ext-json": "*",
        "symfony/console": "^5.2",
        "nikic/php-parser": "^4.10",
        "symfony/dependency-injection": "^5.2",
        "symfony/process": "^5.2",
        "symfony/finder": "^5.2",
        "league/flysystem": "^2.0",
        "symfony/config": "^5.2",
        "symfony/yaml": "^5.2",
        "mockery/mockery": "^1.4"
    },
    "suggest": {
        "jawira/plantuml": "For generating output using PlantUML"
    },
    "autoload": {
        "psr-4": {
            "Jhofm\\PhPuml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhofm\\PhPuml\\Test\\": "tests"
        }
    }
}
